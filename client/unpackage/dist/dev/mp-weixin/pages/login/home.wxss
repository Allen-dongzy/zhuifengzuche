.loginBox {
  width: 224rpx;
  height: 96rpx;
  margin-left: 32rpx;
}
.content {
  background-color: #5A7EFF;
  height: 100vh;
  width: 100%;
}
.contentBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 20vh;
}
.title {
  font-size: 40rpx;
  color: #FFFFFF;
  letter-spacing: 15rpx;
  padding: 8rpx 0rpx;
}
.contentText {
  color: white;
  font-size: 32rpx;
  letter-spacing: 5rpx;
  padding: 8rpx 0rpx;
}
.iconBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.listBox {
  text-align: center;
  padding: 0px 8rpx;
  margin-top: 48rpx;
  color: white;
}
.reg {
  border: 2rpx solid white;
  height: 96rpx;
  width: 40%;
  border-radius: 45rpx;
  display: inline-block;
  text-align: center;
  line-height: 96rpx;
  font-size: 32rpx;
  margin: 0px 5%;
  color: white;
  margin-top: 28vh;
}
.login {
  height: 96rpx;
  width: 40%;
  background-color: white;
  border-radius: 45rpx;
  display: inline-block;
  text-align: center;
  line-height: 96rpx;
  font-size: 32rpx;
  margin: 0px 5%;
  color: #5A7EFF;
}

